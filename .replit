modules = ["nodejs-20", "python-3.11", "postgresql-16"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "PostgreSQL Server"

[[workflows.workflow]]
name = "PostgreSQL Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python database_server.py"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80
